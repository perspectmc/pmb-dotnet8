<pages enableViewState="false" buffer="true"></pages>

@using MBS.Web.Portal.Constants
@model MBS.Web.Portal.Models.ServiceRecordDetailModel

<style>
    /* IE 6 doesn't support max-height
    * we use height instead, but this forces the menu to always be this tall
    */
    .tooltip-inner {
        text-align: left;
    }

    div[class="tooltip-inner"] {
        max-width: 600px;
    }

    * html .ui-autocomplete {
        height: 200px;
    }

    .pad_inline {
        padding-left: 35px;
    }

    .pad_first_inline {
        padding-left: 15px;
    }

    .ui-autocomplete {
        font-size: 10px;
        width: 375px;
        max-height: 200px;
        overflow-y: auto;
        /* prevent horizontal scrollbar */
        overflow-x: hidden;
    }
</style>

