                <div class="form-group pad_inline">
                </div>
            </div>
        </div>

        <div class="form-group">
            <label class="col-sm-3 control-label" for="CommentLabel">Comment</label>
            <div class="form-inline" style="width:1500px">
                @Html.TextAreaFor(m => m.Record.Comment, new { rows = "8", cols = "130", tabIndex = "79" })
                <label class="control-label pad_first_inline">Count</label>
                <input type="text" id="wordCounter" class="form-control" style="width:70px;" onfocus="this.blur();" value="770" />
            </div>
        </div>

        <div class="form-group">
            <label class="col-sm-3 control-label" for="NotesLabel">Personal Notes</label>
            <div class="form-inline" style="width:1500px">
                @Html.TextAreaFor(m => m.Record.Notes, new { rows = "8", cols = "130", tabIndex = "79" })
                <label class="control-label pad_first_inline">Count</label>
                <input type="text" id="wordCounterNotes" class="form-control" style="width:70px;" onfocus="this.blur();" value="1000" />
            </div>
        </div>

        @if ((Model.Record.RejectedClaimId.HasValue || Model.Record.PaidClaimId.HasValue) && !string.IsNullOrEmpty(Model.Record.MessageFromICS))
        {
            <div class="form-group">
                <label class="col-sm-3 control-label">Message From ICS/System</label>
                <div class="form-inline" style="width:1200px">
                    @Html.TextArea("icsmessage", Model.Record.MessageFromICS, new { disabled = "disabled", rows = "8", cols = "90" })
                    @Html.HiddenFor(m => m.Record.MessageFromICS)
                </div>
            </div>
        }

    </div>
</div>

    @Html.HiddenFor(m => m.ButtonUsedToSubmit)
    @Html.HiddenFor(m => m.LastSelectedDiagCode)
    @Html.HiddenFor(m => m.MostUsedCodes)
    @Html.HiddenFor(m => m.PremiumCode)

    <script type="text/javascript">

        $(function () {

            $("#Record_HospitalNumber").focus();

            var isReadOnly = '@(ViewBag.IsAdmin || Model.Record.ClaimStatus == (int)SearchClaimType.Submitted || Model.Record.ClaimStatus == (int)SearchClaimType.Pending || Model.Record.ClaimStatus == (int)SearchClaimType.Paid)' == 'True' ? true : false;
            var codeList = JSON.parse('@Html.Raw(Json.Encode(Model.PremiumCodeList))');
            var careCodeList = JSON.parse('@Html.Raw(Json.Encode(Model.HospitalCareServiceCodeList))');
            var rnpExcludeCodeList = JSON.parse('@Html.Raw(Json.Encode(Model.RNPExcludeCodeList))');
            var versionNumber = '@MBS.DataCache.StaticCodeList.VersionNumber';

            ServiceRecordActionPageLoad(isReadOnly, codeList, careCodeList, rnpExcludeCodeList, versionNumber);
        });

        function SetButtonUsedToSubmit(value) {
            $("#ButtonUsedToSubmit").val(value);
        }
    </script>
